{
  "kafka": {
    "bootstrap_servers": "localhost:9092",
    "no_new_message_limit": 1800,
    "leader_wait_minutes": 30,
    "default_topic_config": {
      "num_partitions": 10,
      "replication_factor": 2,
      "config": {
        "retention.ms": "604800000"
      }
    }
  },
  "bgp_data": {
    "rib_buffer_interval": 1800,
    "data_topic": "ihr_bgp"
  },
  "bgpatom": {
    "full_feed_threshold_v4": 600000,
    "full_feed_threshold_v6": 80000,
    "dump_interval": 900,
    "prefixes_in_atom_batch_size": 50,
    "meta_data_topic": "ihr_bgp_atom_meta",
    "data_topic": "ihr_bgp_atom"
  },
  "bcscore": {
    "dump_interval": 900,
    "meta_data_topic__as": "ihr_bcscore_meta_v6",
    "data_topic__as": "ihr_bcscore_v6",
    "meta_data_topic__prefix": "ihr_bcscore_prefix_meta_v6",
    "data_topic__prefix": "ihr_bcscore_prefix_v6"
  },
  "hege": {
    "dump_interval": 900,
    "data_topic__as": "ihr_hegemony_v6",
    "meta_data_topic__as": "ihr_hegemony_meta_v6",
    "data_topic__prefix": "ihr_hegemony_prefix_v6",
    "meta_data_topic__prefix": "ihr_hegemony_prefix_meta_v6"
  }
}
